<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test_category(test) %></td>
  <td><%= test.author_id %></td>
  <td><%= test.questions.length %></td>
  <td><%= link_to "См.", test %></td>
  <td><%= link_to "Редактировать", edit_test_path(test) %></td>
  <td>
    <%= link_to "Удалить", test_path(test), :data => {:confirm => "Вы уверены?"}, :method => :delete %>
  </td>
</tr>
